#include<iostream>


class Person {
    protected:
        std::string firstname;
        std::string lastname;
        int age;
    public:
        Person(string name,string lastname, int age) {
            name = name;
            lastname = lastname;
            age = age;

        }
};

class Student : public Person {
    private:
        std::string institution;
        int year of study;
        std::string Registration no;
    public:
        Student(string name,string lastname, int age, string institution, int year of study, string Registration no) : Person(name,lastname, age) {
            institution = institution;
            year of study = year of study;
            Registration no = Registration no;
        }
};
